{
    "caption": "Common Vulnerabilities and Exposures",
    "description": "CVE object represents publicly disclosed cybersecurity vulnerabilities defined in CVE Program catalog. There is one CVE Record for each vulnerability in the catalog.",
    "extends": "object",
    "name": "cve",
    "attributes": {
        "cve_uid": {
            "requirement": "required"
        },
        "cvss": {
            "requirement": "recommended"
        },
        "vulnerability_type": {
            "description": "Vulnerability Type",
            "enum": {
                "Unknown": {
                    "caption": "Unknown"
                },
                "Denial of Service": {
                    "caption": "Denial of Service (DoS)"
                },
                "Code Execution": {
                    "caption": "Code Execution"
                },
                "Overflow": {
                    "caption": "Overflow"
                },
                "Memory Corruption": {
                    "caption": "Memory Corruption"
                },
                "Sql Injection": {
                    "caption": "Sql Injection"
                },
                "Cross Site Scripting": {
                    "caption": "Cross Site Scripting (XSS)"
                },
                "Directory Traversal": {
                    "caption": "Directory Traversal"
                },
                "Http Response Splitting": {
                    "caption": "Http Response Splitting"
                },
                "Bypass Something": {
                    "caption": "Bypass Something"
                },
                "Gain Information": {
                    "caption": "Gain Information"
                },
                "Gain Privileges": {
                    "caption": "Gain Privileges"
                },
                "Cross Site Request Forgery": {
                    "caption": "Cross Site Request Forgery (CSRF)"
                },
                "File Inclusion": {
                    "caption": "File Inclusion"
                },
                "Other": {
                    "caption": "Other"
                }
            },
            "requirement": "recommended"
        },
        "cwe_id": {
            "requirement": "optional"
        },
        "cwe_url": {
            "requirement": "optional"
        },
        "published_time": {
            "requirement": "optional"
        },
        "modified_time": {
            "requirement": "optional"
        },
        "source_vendor": {
            "requirement": "optional"
        }
    }
}