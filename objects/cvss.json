{
  "description": "The CVSS object details Common Vulnerability Scoring System (CVSS) scores from the advisory that are related to the vulnerability.",
  "caption": "CVSS Scores",
  "name": "cvss",
  "extends": "object",
  "attributes": {
    "cvss_score": {
      "description": "The CVSS score value",
      "requirement": "recommended"
    },
    "cvss_vector_string": {
      "description": "The CVSS vector string is a text representation of a set of CVSS metrics. It is commonly used to record or transfer CVSS metric information in a concise form. An example: CVSS:3.1/AV:N/AC:L/PR:H/UI:N/S:U/C:L/I:L/A:N",
      "requirement": "optional"
    },
    "cvss_depth_id": {
      "description": "The CVSS depth. Representing a depth of the equation used to calculate CVSS score.",
      "enum": {
        "0": {
          "caption": "Base"
        },
        "1": {
          "caption": "Temporal"
        },
        "2": {
          "caption": "Environmental"
        }
      },
      "requirement": "recommended"
    },
    "cvss_severity_id": {
      "description": "A textual representation of the numeric score. None (0.0), Low (0.1 - 3.9), Medium (4.0 - 6.9), High (7.0 - 8.9), Critical (9.0 - 10.0)",
      "enum": {
        "0": {
          "caption": "None"
        },
        "1": {
          "caption": "Low"
        },
        "2": {
          "caption": "Medium"
        },
        "3": {
          "caption": "High"
        },
        "4": {
          "caption": "Critical"
        }
      },
      "requirement": "optional"
    },
    "cvss_version": {
      "description": "The CVSS version (v1, v2, v3)",
      "requirement": "recommended"
    },
    "cvss_access_complexity_id": {
      "description": "Name: Access Complexity (AC). Group: Base. CVSS Version: v1, v2",
      "enum": {
        "0": {
          "name": "Low (L)"
        },
        "1": {
          "name": "Medium (M)"
        },
        "2": {
          "name": "High (H)"
        }
      },
      "requirement": "optional"
    },
    "cvss_attack_complexity_id": {
