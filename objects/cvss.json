{
  "description": "The CVSS object details Common Vulnerability Scoring System (CVSS) scores from the advisory that are related to the vulnerability.",
  "name": "CVSS Scores",
  "type": "cvss",
  "extends": "object",
  "attributes": {
    "access_complexity_id": {
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Low"
        },
        "2": {
          "name": "Medium"
        },
        "3": {
          "name": "High"
        }
      },
      "requirement": "optional"
    },
    "attack_vector_id": {
      "enum": {
        "-1": {
          "name": "Unknown"
        },
        "0": {
          "name": "Physical"
        },
        "1": {
          "name": "Local Access"
        },
        "2": {
          "name": "Adjacent Network Access"
        },
        "3": {
          "name": "Network Accessible"
        }
      },
      "requirement": "optional"
    },
    "authentication_id": {
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "Multiple Instances"
        },
        "2": {
          "name": "Single Instance"
        },
        "3": {
          "name": "No Authentication"
        }
      },
      "requirement": "optional"
    },
    "availability_impact_id": {
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "No Impact"
        },
        "2": {
          "name": "Partial"
        },
        "3": {
          "name": "Complete"
        }
      },
      "requirement": "optional"
    },
    "base_score": {
      "requirement": "recommended"
    },
    "base_vector": {
      "requirement": "recommended"
    },
    "confidentiality_impact_id": {
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "No Impact"
        },
        "2": {
          "name": "Partial"
        },
        "3": {
          "name": "Complete"
        }
      },
      "requirement": "optional"
    },
    "integrity_impact_id": {
      "enum": {
        "0": {
          "name": "Unknown"
        },
        "1": {
          "name": "No Impact"
        },
        "2": {
          "name": "Partial"
        },
        "3": {
          "name": "Complete"
        }
      },
      "requirement": "optional"
    },
    "risk": {
      "requirement": "optional"
    },
    "version": {
      "description": "The CVSS version",
      "requirement": "recommended"
    }, 
    "privileges_required_id": {
      "enum": {
        "0": {
          "name": "None"
        },
        "1": {
          "name": "Low"
        },
        "2": {
          "name": "High"
        }
      },
      "requirement": "optional"
    },
    "user_interaction_id": {
      "enum": {
        "0": {
          "name": "None"
        },
        "1": {
          "name": "Required"
        }
      },
      "requirement": "optional"
    },
    "attack_complexity_id": {
      "enum": {
        "0": {
          "name": "Low"
        },
        "1": {
          "name": "High"
        }
      },
      "requirement": "optional"
    }
  }
}
