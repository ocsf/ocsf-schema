{
  "description": "The attributes that identify security controls such as malware or policy violations.",
  "meta": "profile",
  "caption": "Security Control",
  "name": "security_control",
  "annotations": {
    "group": "primary"
  },
  "attributes": {
    "attacks": {
      "requirement": "recommended"
    },
    "disposition": {
      "requirement": "optional"
    },
    "disposition_id": {
      "requirement": "required",
      "enum": {
        "1": {
          "caption": "Allowed"
        },
        "2": {
          "caption": "Blocked"
        },
        "3": {
          "caption": "Quarantined"
        },
        "4": {
          "caption": "Isolated"
        },
        "5": {
          "caption": "Deleted"
        },
        "6": {
          "caption": "Dropped"
        },
        "7": {
          "caption": "Custom Action",
          "description": "Executed custom action such as run a command script."
        },
        "8": {
          "caption": "Approved"
        },
        "9": {
          "caption": "Restored"
        },
        "10": {
          "caption": "Exonerated",
          "description": "No longer suspicious (re-scored)."
        },
        "11": {
          "caption": "Corrected"
        },
        "12": {
          "caption": "Partially Corrected"
        },
        "13": {
          "caption": "Uncorrected"
        },
        "14": {
          "caption": "Delayed",
          "description": "Requires reboot to finish the operation."
        },
        "15": {
          "caption": "Detected"
        },
        "16": {
          "caption": "No Action"
        },
        "17": {
          "caption": "Logged"
        },
        "18": {
          "caption": "Tagged",
          "description": "Marked with extended attributes."
        }
      }
    },
    "kill_chain": {
      "requirement": "optional"
    },
    "malware": {
      "requirement": "optional"
    }
  }
}
